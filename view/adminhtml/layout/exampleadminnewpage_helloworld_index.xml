<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">PriorNotify plugin</argument>
        </action>
    </referenceBlock>
    <head>
        <css src="Mageplaza_HelloWorld::css/priorNotify.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Backend\Block\Template" template="Mageplaza_HelloWorld::helloworld.phtml"/>
        </referenceContainer>
    </body>
</page>